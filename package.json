{
  "name": "react-native-mapsforge-vtm",
  "version": "0.1.3",
  "description": "React Native components to build vector maps using Mapsforges fork of vtm. Offline rendering of OpenStreetMap data. Android only",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "cpp",
    "*.podspec",
    "react-native.config.json",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-mapsforge-vtm-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build example/android/build example/android/app/build lib",
    "prepare": "bob build",
    "publish": "bash ./scripts/publish.sh"
  },
  "keywords": [
    "react-native",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhotadhari/react-native-mapsforge-vtm.git"
  },
  "author": "jhotadhari <tellme@waterproof-webdesign.de> (https://github.com/jhotadhari)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhotadhari/react-native-mapsforge-vtm/issues"
  },
  "homepage": "https://github.com/jhotadhari/react-native-mapsforge-vtm#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native/eslint-config": "^0.73.1",
    "@types/jest": "^29.5.5",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.44",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "keep-a-changelog": "^2.5.3",
    "prettier": "^3.0.3",
    "react": "18.3.1",
    "react-native": "0.76.0",
    "react-native-builder-bob": "^0.30.3",
    "turbo": "^1.10.7",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "view-legacy",
    "languages": "kotlin-swift",
    "version": "0.42.2"
  },
  "dependencies": {
    "events": "^3.3.0",
    "lodash-es": "^4.17.21",
    "queue-promise": "^2.2.1"
  }
}
